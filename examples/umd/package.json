{
  "name": "alien-umd-project",
  "version": "1.0.0",
  "author": "Patrick Schroen",
  "private": true,
  "main": "build/project.js",
  "scripts": {
    "build": "rollup -c || exit 0",
    "dev": "concurrently --names \"ROLLUP,HTTP\" -c \"bgBlue.bold,bgGreen.bold\" \"rollup -c -w -m inline\" \"serve -l 8080 public\" || exit 0",
    "start": "npm run dev",
    "lint": "eslint src || exit 0",
    "pngquant": "rm -rf public/assets/images && cp -r src/images public/assets/ && find public/assets/images -name '**.png' -exec pngquant --strip --ext=.png --force {} \\;"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint": "^5.8.0",
    "replace": "^1.0.0",
    "rollup": "0.55.3",
    "rollup-plugin-bundleutils": "^1.0.8",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-glslify": "^1.0.7",
    "rollup-watch": "^4.3.1",
    "serve": "^10.0.2"
  }
}
