{
  "name": "alien.js",
  "version": "0.14.0",
  "description": "Future web framework",
  "homepage": "https://alien.js.org/",
  "keywords": [
    "alien",
    "alienjs",
    "javascript",
    "html5",
    "css3",
    "animation",
    "webgl",
    "threejs",
    "webaudio"
  ],
  "author": "Patrick Schroen",
  "repository": {
    "type": "git",
    "url": "https://github.com/pschroen/alien.js"
  },
  "bugs": {
    "url": "https://github.com/pschroen/alien.js/issues"
  },
  "license": "MIT",
  "main": "build/alien.js",
  "scripts": {
    "build": "rollup -c && npm run build-uglify",
    "build-uglify": "rollup -c --environment uglify",
    "build-examples": "for x in `find examples -name 'rollup.config.js' -maxdepth 2 ! -path 'alien.js/*' ! -path 'examples/npm/*' -exec dirname {} \\;`; do cd \"$x\" && ln -sfn ../.. alien.js && rollup -c && rm -f alien.js && cd ../..; done",
    "build-world": "npm run build && npm run build-examples",
    "start": "serve -l 8080 || exit 0",
    "lint": "eslint --ignore-pattern 'src/gsap/*' src || exit 0"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.9.0",
    "replace": "^1.0.0",
    "rollup": "^0.67.3",
    "rollup-plugin-bundleutils": "^1.0.9",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-glslify": "^1.0.7",
    "rollup-watch": "^4.3.1",
    "serve": "^10.1.1"
  }
}
